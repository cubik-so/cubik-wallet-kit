{
    "extends": "@cubik-wallet/configs/tsconfig/index.json",
    "compilerOptions": {
        "jsx": "react-jsx",
        "module": "esnext",
        "baseUrl": "./src",
        "moduleResolution": "bundler",
        "paths": {
            "@/*": ["./src/*"],
            "@/lib/*": ["./src/lib/*"],
            "@/context/*": ["./src/context/*"],
            "@/hooks/*": ["./src/hooks/*"],
            "@/components/*": ["./src/components/*"],
            "@/utils/*": ["./src/utils/*"]
        }
    }
}
